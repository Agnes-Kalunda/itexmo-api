{
    "name": "agnes/itexmo-sms",
    "description": "Itexmo SMS Gateway Integration for Laravel 5.8",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Agnes",
            "email": "agypeter97@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.8.*",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "orchestra/testbench": "3.8.*"
    },
    "autoload": {
        "psr-4": {
            "Agnes\\ItexmoSms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Agnes\\ItexmoSms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Agnes\\ItexmoSms\\ItexmoSmsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable"
}